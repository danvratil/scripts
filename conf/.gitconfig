	email = dvratil@kde.org
	name = Daniel Vr√°til
[url "git@git.kde.org:"]
	pushInsteadOf = kde:
[url "https://github.com/"]
	insteadOf = github:
[url "git@github.com:"]
	pushInsteadOf = github:

[init]
	templatedir = ~/.git-templates
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
[url "https://anongit.kde.org/"]
    insteadOf = kde:

[url "git@git.int.avast.com:"]
        insteadOf = avast:
        pushInsteadOf = avast:
[mergetool]
        prompt = false

[merge]
	tool = vimdiff
[git-pull-request]
	fork = never
[hub]
	host = git.int.avast.com
[alias]
	rbi = rebase -i --autosquash
	fixup = !git commit --fixup $(git-commit-base)
